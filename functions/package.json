{
  "name": "parking-app-functions",
  "version": "1.0.0",
  "description": "Firebase Functions for Event Parking App",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
