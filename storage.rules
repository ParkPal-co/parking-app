service firebase.storage {
  match /b/{bucket}/o {
    match /parkingSpots/{userId}/{fileName} {
      allow read: if true;
      allow write: if request.auth != null 
                   && request.auth.uid == userId;
    }
    
    match /profile-images/{userId}/{fileName} {
      allow read: if true;
      allow write: if request.auth != null 
                   && request.auth.uid == userId;
    }
  }
}